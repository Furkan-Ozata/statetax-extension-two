{
  "name": "AutofillExtension",
  "description": "Autofill stuff",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "welcome/background.js"
  },
  "action": {
    "default_popup": "ext/index.html",
    "default_icon": "./48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "form.js", "createUser.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "declarativeContent"]
}
